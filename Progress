import java.io.Serializable;

public class Progress implements Serializable {

    private int health;
    private int armor;
    private String place;
    private int level;

    public Progress (int health,int armor,String place,int level){
        this.health=health;
        this.armor=armor;
        this.place=place;
        this.level=level;
    }

    public int getHealth(){
        return health; }

    public int getArmor(){
        return armor; }

    public String getPlace(){
        return place; }

    public int getLevel(){
        return level; }
}
